<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SCMS.Controllers.RemoveStudentController">
    <children>

        <Label fx:id="titleText" alignment="CENTER" layoutX="88.0" layoutY="20.0" prefHeight="49.0" prefWidth="475.0" text="Remove a Student from club" />

        <TextField fx:id="deleteStudentId" layoutX="207.0" layoutY="128.0" prefHeight="27.0" prefWidth="279.0" promptText="Student ID" />
        <Button fx:id="removeStudentButton" layoutX="231.0" layoutY="344.0" mnemonicParsing="false" onAction="#onRemoveStudentButtonClick" prefHeight="24.0" prefWidth="146.0" text="Remove" />
        <Label layoutX="70.0" layoutY="133.0" text="Enter Student Id" textFill="WHITE" />
        <Label layoutX="70.0" layoutY="203.0" text="Enter Club Id" textFill="WHITE" />
        <TextField fx:id="deleteStudentClub" layoutX="207.0" layoutY="198.0" prefHeight="27.0" prefWidth="279.0" promptText="Club ID" />
        <TextField fx:id="deleteStudentReason" layoutX="207.0" layoutY="270.0" prefHeight="27.0" prefWidth="279.0" promptText="Reason" />
        <Label layoutX="70.0" layoutY="275.0" text="Enter Reason" textFill="WHITE" />
        <Label fx:id="statusShowLabel" alignment="CENTER" layoutX="126.0" layoutY="409.0" prefHeight="28.0" prefWidth="283.0" />
        <Label fx:id="studentIdStatus" layoutX="207.0" layoutY="163.0" prefHeight="18.0" prefWidth="279.0" />
        <Label fx:id="clubIdStatus" layoutX="207.0" layoutY="230.0" prefHeight="18.0" prefWidth="279.0" />
        <Label fx:id="reasonStatus" layoutX="207.0" layoutY="306.0" prefHeight="18.0" prefWidth="279.0" />
      <Button fx:id="backButtonCDD" layoutX="48.0" layoutY="31.0" mnemonicParsing="false" onAction="#backButtonCDD" prefHeight="26.0" prefWidth="40.0" text="&lt;--" />
    </children>
   <stylesheets>
      <URL value="@../StylingSheets/DeleteStudent.css" />
      <URL value="@../StylingSheets/rootStyles.css" />
   </stylesheets>
</AnchorPane>
